<TreeViewItem x:Class="UIEditor.FileMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Name="mx_root">
	<TreeViewItem.ContextMenu>
		<ContextMenu Name="mx_menu">
			<MenuItem Name="mx_copyFile" Header="复制" InputGestureText="Ctrl+C" IsEnabled="False"/>
			<MenuItem Name="mx_paste" Header="粘贴" InputGestureText="Ctrl+V" IsEnabled="False"/>
			<MenuItem Name="mx_rename" Header="重命名" InputGestureText="???" IsEnabled="False"/>
		</ContextMenu>
	</TreeViewItem.ContextMenu>
</TreeViewItem>
